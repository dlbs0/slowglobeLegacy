{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "../engine/src/**/*",
    "../engine/src/**/*.vue",
    "../trips/**/*",
    "../trips/**/*.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["../engine/src/*"],
      "~/*": ["../trips/*"]
    }
  }
}
